{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dftempdev12"
		},
		"ls_adls_creportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_creportingdl'"
		},
		"ls_blob_creportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_creportingsa'"
		},
		"ls_adls_creportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://creportingdl.dfs.core.windows.net"
		},
		"ls_http_opendata_ecdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"trigger_cpy_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/04359d4b-3995-454e-9dd5-88466e9aa26e/resourceGroups/dftemodev/providers/Microsoft.Storage/storageAccounts/creportingsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dftempdev12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_creportingdl_accountKey": {
						"value": "[parameters('ls_adls_creportingdl_accountKey')]"
					},
					"ls_blob_creportingsa_connectionString": {
						"value": "[parameters('ls_blob_creportingsa_connectionString')]"
					},
					"ls_adls_creportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_creportingdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_properties_typeProperties_url')]"
					},
					"trigger_cpy_properties_typeProperties_scope": {
						"value": "[parameters('trigger_cpy_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}